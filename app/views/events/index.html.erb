<h1>Listing events</h1>
<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>Fromdate</th>
			<th>Fromtime</th>
			<th>Untiltime</th>
			<th>Untildate</th>
			<th>Address1</th>
			<th>Address2</th>
			<th>Zip</th>
			<th>Description</th>
			<th>Url</th>
			<th>Email</th>
			<th>Phone</th>
			<th>User</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @events.each do |event| %>
			<tr>
				<td><%= event.title %></td>
				<td><%= event.fromDate %></td>
				<td><%= event.fromTime %></td>
				<td><%= event.untilTime %></td>
				<td><%= event.untilDate %></td>
				<td><%= event.address1 %></td>
				<td><%= event.address2 %></td>
				<td><%= event.zip %></td>
				<td><%= event.description %></td>
				<td><%= event.url %></td>
				<td><%= event.email %></td>
				<td><%= event.phone %></td>
				<td><%= event.user %></td>
				<td><%= link_to 'Show', event %></td>
				<td><%= link_to 'Edit', edit_event_path(event) %></td>
				<td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
			</tr>
		<% end %>
	</tbody>
</table>

